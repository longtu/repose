curl -H "Content-Type: application/json" -X POST -d '{
  "farmURl": "abc.com",
  "farmName": "csd",
  "list_of_users_id":[
    "123",
    "456"
  ]
}' http://localhost:8080/userfarms -H "X-User-Name:123"


curl -H "Content-Type: application/json" -X POST -d '{
  "farmId": "68",
  "dstIp": "jsonplaceholder.typicode.com",
  "dstPortnumber": 80,
  "gatewaySubDir": ".*/nova.*",
  "rateLimiting":[
    {"uri":"*", "uriRegex":"/(.*)/1", "httpMethods":"GET", "unit": "hour", "value": 1}, 
    {"uri":"/posts/2", "uriRegex":"/posts/2", "httpMethods":"GET", "unit": "minute", "value": 2},
    {"uriRegex":"/.*", "httpMethods":"GET", "unit": "second", "value": 5}
  ]
 
}' http://localhost:8080/userfarms/userinput -H "X-User-Name:123"

curl http://localhost:8080/userfarms/68 -H "X-User-Name:123"

curl http://localhost:8080/userfarms/68/system-model.cfg.xml
